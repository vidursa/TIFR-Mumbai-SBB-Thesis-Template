@article{vernos1996,
  title = {Motors Involved in Spindle Assembly and Chromosome Segregation},
  author = {Vernos, I. and Karsenti, E.},
  year = {1996},
  month = feb,
  journal = {Current Opinion in Cell Biology},
  volume = {8},
  number = {1},
