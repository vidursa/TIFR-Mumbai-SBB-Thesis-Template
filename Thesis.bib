@article{gaspar2017,
  title = {An {{RNA-binding}} Atypical Tropomyosin Recruits Kinesin-1 Dynamically to Oskar {{mRNPs}}},
  author = {G{\'a}sp{\'a}r, Imre and Sysoev, Vasiliy and Komissarov, Artem and Ephrussi, Anne},
  year = {2017},
