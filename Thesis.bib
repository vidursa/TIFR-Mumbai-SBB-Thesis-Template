  keywords = {*Ubiquitination,Animals,Gene Expression Regulation/immunology,Humans,NF-kappa B/*metabolism,Protein Binding,Signal Transduction/immunology,Structure-Activity Relationship,Transcriptional Activation/immunology,Ubiquitins/*immunology}
