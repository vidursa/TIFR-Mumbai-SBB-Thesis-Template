  journal = {Experimental Neurology},
  volume = {57},
  number = {1},
  pages = {282--295},
  issn = {0014-4886},
  doi = {10.1016/0014-4886(77)90064-4},
  urldate = {2023-07-11},
  abstract = {The composition of major protein components of myelin-free axons isolated from selected peripheral nerves of the rabbit was determined by sodium dodecyl sulfate microelectrophoresis. Five major peaks were identified from microdensitometric profiles of microelectrophoregrams, having apparent molecular weights in daltons of 620,000, 200,000, 100,000, 62,000 and 52,000. The major 100,000-dalton component was converted in situ to products in the 60,000 to 50,000-dalton range by in vitro incubation in elevated calcium, a property characteristic of neurofilament protein. Proteins labeled in myelinated axons of the hypoglossal nerve had apparent molecular weights of 100,00, 52,000, and 18,000 daltons. Indirect evidence suggested that neurofilament protein may be a product synthesized by the myelinated axon locally.},
  langid = {english}
}

@article{friedlander2000,
  title = {A Regulatory Link between {{ER-associated}} Protein Degradation and the Unfolded-Protein Response.},
  author = {Friedlander, Ruth and Jarosch, Ernst and Urban, J{\"o}rg and Volkwein, Corinna and Sommer, Thomas},
