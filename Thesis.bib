  pages = {187--196},
  issn = {1463-6395},
  doi = {10.1111/j.1463-6395.1979.tb00611.x},
  urldate = {2023-07-17},
  abstract = {Ultrastructural and histochemical studies indicate a neurosecretory system exists in the lancelet brain with basal properties resembling a primitive hypothalamic system. A nucleus of secreting neurons, containing peptide granules (115 nm), is prominent in the dorsal walls of the brain. The axons establish contacts with the ventral brain surface, probably releasing their secretory product out of the brain. The neurons are innervated by dopaminergic ``boutons en passant'' often very active with a high number of electron translucent vesicles as well as dense-core vesicles (90 nm). Ventrally located cellbodies containing what are probably secretory peptide granules (110 nm) establish contacts with their basal processes on the ventral brain surface.},
  copyright = {1979 The Royal Swedish Academy of Sciences},
  langid = {english}
}

@article{ochs1969,
  title = {Characteristics of the Fast Transport System in Mammalian Nerve Fibers},
  author = {Ochs, S. and Ranish, N.},
  year = {1969},
  journal = {Journal of Neurobiology},
  volume = {1},
  number = {2},
  pages = {247--261},
  issn = {1097-4695},
  doi = {10.1002/neu.480010211},
  urldate = {2023-07-19},
  abstract = {The rate of the fast transport system in cat sensory nerve fibers shown by the distal displacement of a peak of isotopic activity in sciatic nerves after L7 ganglion injection with 3H-leucine, is 401 {$\pm$} 35 mm/day. A similar rate of fast transport was also found for motor fibers in a smaller number of studies. An intra-axonic locus of the labelled material was determined by freeze-block experiments. Fast transport was differentiated from the slow mechanism of axoplasmic flow by injecting 3H-leucine into one L7 ganglion and 32P-orthophosphate into the opposite one of the same animals, the labelled components of the latter moving down the nerve at the much slower rate characteristic of slow axoplasmic transport. The mechanism underlying fast transport is locally present all along the nerve fibers; the soma is not responsible for propulsion. This was shown by ``pulse'' experiments where ligations were made just distal to the injected ganglion at times from 1\textendash 3 hrs afterwards and the same rate of outward movement of labelled materials was found in these fibers without somal continuity.},
  copyright = {Copyright \textcopyright{} 1969 John Wiley \& Sons, Inc.},
  langid = {english},
  keywords = {*Axonal Transport,Animals,Cats,Freezing,{Ganglia, Spinal},Injections,Interneurons/analysis,Leucine/analysis/metabolism,Ligation,Motor Neurons/*metabolism,Nerve Tissue Proteins/metabolism,Phosphates/metabolism,Phosphorus Isotopes,Sciatic Nerve/*metabolism,Spinal Nerve Roots,Time Factors,Tritium}
}

